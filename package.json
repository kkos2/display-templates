{
  "name": "templates",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci",
    "lint:js": "eslint --ext .js --ext .jsx ./src",
    "lint:js:fix": "eslint --ext .js --ext .jsx --fix ./src",
    "lint:scss": "stylelint \"./src/**/*.scss\"",
    "lint:scss:fix": "stylelint --fix \"./src/**/*.scss\"",
    "lint:ts": "eslint --ext .ts --ext .tsx ./src",
    "lint:ts:fix": "eslint --ext .ts --ext .tsx --fix ./src",
    "check-coding-standards": "npm run lint:js && npm run lint:scss",
    "apply-coding-standards": "npm run lint:js:fix && npm run lint:scss:fix",
    "build": "NODE_ENV=production webpack",
    "build-watch": "NODE_ENV=production webpack --watch",
    "start": "NODE_ENV=development webpack serve --mode development --env development",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o storybook/storybook-static"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "testMatch": ["**/?(*.)(spec|test).js?(x)"]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.15.5",
